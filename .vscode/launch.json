{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Go: Launch Gin (Debug)",
      "type": "go",
      "request": "launch",
      "program": "main.go",
      "mode": "debug",
      "env": {
        "GIN_MODE": "debug"
      },
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "Listening and serving HTTP on :([0-9]+)",
        "uriFormat": "http://localhost:%s/swagger/index.html"
      }
    }
  ]
}
